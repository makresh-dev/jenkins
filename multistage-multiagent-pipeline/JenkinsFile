pipeline {
    agent none
    stages('backend'){
        agent {
            docker { inmage 'maven:3.8.1-adoptopenjdk-11' }
        }
        steps {
            sh 'mvn --version'
        }
    }
    stages('frontend'){
        agent {
            docker { image 'node:16-alpine'}
        }
        steps {
            sh 'node --version'
        }
    }
}